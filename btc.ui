     <string notr="true">QComboBox {
    selection-background-color: rgba(100, 100, 100, 0.8); /* Background color when an item is selected */
    selection-color: rgb(255, 255, 255); /* Text color when an item is selected (white) */
    color: rgb(255, 255, 255); /* Default text color (white) */
    border: 2px solid #555; /* Darker border for better contrast */
    border-radius: 7px; /* Rounded corners */
    background: rgba(50, 50, 50, 0.9); /* Dark gray semi-transparent background */
    padding: 5px; /* Increased padding for better spacing */
    font-size: 20px; /* Font size */
}

QComboBox:hover {
    border-color: rgba(150, 150, 150, 0.9); /* Change border color on hover */
}

QComboBox::drop-down {
    border: none; /* No border for dropdown button */
    background: rgba(50, 50, 50, 0.9); /* Same background as the combo box */
}

QComboBox::down-arrow {
    image: url(path/to/your/icon.png); /* Custom arrow icon */
    width: 12px; /* Width of the arrow icon */
    height: 12px; /* Height of the arrow icon */
}

QComboBox::drop-down:hover {
    background: rgba(70, 70, 70, 0.7); /* Background color when hovering over dropdown button */
}

/* Styles for QAbstractItemView when the ComboBox is opened */
QComboBox QAbstractItemView {
    background: rgba(60, 60, 60, 0.9); /* Background color for dropdown items */
    color: rgb(255, 255, 255); /* Text color for dropdown items (white) */
    selection-background-color: rgba(100, 100, 100, 0.8); /* Background color for selected item */
    selection-color: rgb(255, 255, 255); /* Text color for selected item (white) */
}

/* Changes for active (open) state */
QComboBox:focus {
    background: rgba(70, 70, 70, 0.9); /* Background color when focused (open) */
    color: rgb(255, 255, 255); /* Text color when focused (open) */
}

QComboBox:disabled {
    color: rgba(255, 255, 255, 0.5); /* Text color when disabled (light gray) */
    border-color: rgba(190, 190, 190, 0.3); /* Lighter border when disabled */
}
